prompt ============================
prompt
create table aip_role_desk
(
  ROLE_ID           NVARCHAR2(36) not null,
  PERMISSION_ID     NVARCHAR2(36) not null,
  TYPE              NVARCHAR2(30),
  CREATED_BY        NVARCHAR2(36) not null,
  Created_Date      DATE not null,
  LAST_UPDATED_BY   NVARCHAR2(36) not null,
  Last_Updated_Date DATE not null,
  e_code            NVARCHAR2(50) not null
)
;
comment on table aip_role_desk
  is '角色桌面权限';
comment on column aip_role_desk.ROLE_ID
  is '角色标识';
comment on column aip_role_desk.PERMISSION_ID
  is '操作标识';
comment on column aip_role_desk.TYPE
  is '类型';
comment on column aip_role_desk.CREATED_BY
  is '创建人';
comment on column aip_role_desk.Created_Date
  is '创建时间';
comment on column aip_role_desk.LAST_UPDATED_BY
  is '最后修改人';
comment on column aip_role_desk.Last_Updated_Date
  is '最后修改时间';
comment on column aip_role_desk.e_code
  is '企业编码';
alter table aip_role_desk
  add primary key (ROLE_ID, PERMISSION_ID);

