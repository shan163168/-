prompt Creating table app_rank_log
prompt ===========================
prompt
create table app_rank_log
(
  id            NVARCHAR2(38) not null,
  target_id     NVARCHAR2(38),
  target_name   NVARCHAR2(50),
  operator_id   NVARCHAR2(38),
  operator_name NVARCHAR2(50),
  record_time   DATE,
  event_time    DATE,
  e_code        NVARCHAR2(50)
)
;
COMMENT ON COLUMN app_rank_log.id IS '记录ID';
COMMENT ON COLUMN app_rank_log.target_id IS '目标ID';
COMMENT ON COLUMN app_rank_log.target_name IS '目标名称';
COMMENT ON COLUMN app_rank_log.operator_id IS '操作者ID';
COMMENT ON COLUMN app_rank_log.operator_name IS '操作者名称';
COMMENT ON COLUMN app_rank_log.record_time IS '记录时间';
COMMENT ON COLUMN app_rank_log.event_time IS '事件时间';
COMMENT ON COLUMN app_rank_log.e_code IS 'E码';

alter table app_rank_log
  add primary key (id);

