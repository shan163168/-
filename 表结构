COMMENT ON COLUMN msg_message.from_type IS '发送者类型';
COMMENT ON COLUMN msg_message.to_id IS '接收者ID';
COMMENT ON COLUMN msg_message.to_name IS '接收者姓名';
COMMENT ON COLUMN msg_message.to_type IS '接收者类型';
COMMENT ON COLUMN msg_message.content IS '消息内容';
COMMENT ON COLUMN msg_message.read IS '是否已读';
COMMENT ON COLUMN msg_message.user_id IS '用户ID';
COMMENT ON COLUMN msg_message.send_time IS '发送时间';
COMMENT ON COLUMN msg_message.rec_time IS '接收时间';
COMMENT ON COLUMN msg_message.msg_id IS '关联消息ID';

comment on column msg_message.e_code
  is '企业编码';
