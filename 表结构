(
  DUTY_ID           NVARCHAR2(36) not null,
  DUTY_NUM          NVARCHAR2(50),
  NAME              NVARCHAR2(255) not null,
  PARENT_ID         NVARCHAR2(36),
  TYPE_Y              NUMBER(6),
  STATUS            NUMBER(11),
  ORDER_NO          NUMBER(11) not null,
  DESCRIPTION       NVARCHAR2(1000),
  CREATED_DATE      DATE,
  LAST_UPDATED_DATE DATE,
  CREATED_BY        NVARCHAR2(36),
  LAST_UPDATED_BY   NVARCHAR2(36),
  e_code            NVARCHAR2(50) not null
)
;
