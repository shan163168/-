prompt
prompt Creating table app_rank_user_app_day
prompt ====================================
prompt
create table app_rank_user_app_day
(
  id           NVARCHAR2(38) not null,
  user_id      NVARCHAR2(38),
  user_name    NVARCHAR2(50),
  user_num     NVARCHAR2(50),
  picture      NVARCHAR2(300),
  org_id       NVARCHAR2(38),
  org_name     NVARCHAR2(255),
  parent_id    NVARCHAR2(36),
  app_id       NVARCHAR2(38),
  app_name     NVARCHAR2(255),
  icon         NVARCHAR2(300),
  click_volume NUMBER(11),
  message_num  NUMBER(11),
  blog_num     NUMBER(11),
  login_num    NUMBER(11),
  total        NUMBER(11),
  record_time  DATE,
  event_time   DATE
)
;
comment on table app_rank_user_app_day
  is '每个用户每个应用的点击量，时间精度为一天';
comment on column app_rank_user_app_day.id
  is 'ID';
comment on column app_rank_user_app_day.user_id
  is '用户ID';
comment on column app_rank_user_app_day.user_name
  is '用户姓名';
comment on column app_rank_user_app_day.user_num
  is '警号';
comment on column app_rank_user_app_day.picture
  is '用户头像';
comment on column app_rank_user_app_day.org_id
  is '部门ID';
comment on column app_rank_user_app_day.org_name
  is '部门名称';
comment on column app_rank_user_app_day.app_id
  is '应用ID';
comment on column app_rank_user_app_day.app_name
  is '应用名称';
comment on column app_rank_user_app_day.icon
  is '应用图标';
comment on column app_rank_user_app_day.click_volume
  is '点击量（日）';
comment on column app_rank_user_app_day.message_num
  is '消息数';
comment on column app_rank_user_app_day.blog_num
  is '动态数';
comment on column app_rank_user_app_day.login_num
  is '登录数';
comment on column app_rank_user_app_day.total
  is '总计';
comment on column app_rank_user_app_day.record_time
  is '时间（日）';
comment on column app_rank_user_app_day.event_time
  is '发生时间（日）';
alter table app_rank_user_app_day
  add primary key (id);

