create table msg_message
(
  id        NVARCHAR2(40) not null,
  msg_type  NUMBER(11),
  from_id   NVARCHAR2(40),
  from_name NVARCHAR2(100),
  from_type NUMBER(11),
  to_id     NVARCHAR2(40),
  to_name   NVARCHAR2(100),
  to_type   NUMBER(11),
  content   NCLOB,
  read      NUMBER(4),
  user_id   NVARCHAR2(40),
  send_time DATE,
  rec_time  DATE,
  msg_id    NVARCHAR2(40),
  e_code    NVARCHAR2(50) not null
)
;
