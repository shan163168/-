create table msg_message_article
(
  id                NVARCHAR2(36) not null,
  title             NVARCHAR2(200),
  task_id           NVARCHAR2(36),
  msg_id            NVARCHAR2(36) not null,
  msg_index         NUMBER(11) not null,
  total_read        NUMBER(11),
  total_like        NUMBER(11),
  comment_count     NUMBER(11),
  serviceno_id      NVARCHAR2(36),
  extra_attributes  NVARCHAR2(200),
  created_by        NVARCHAR2(36),
  created_date      DATE,
  last_updated_by   NVARCHAR2(36),
  last_updated_date DATE
)
;
