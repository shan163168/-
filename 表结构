prompt ============================
prompt
create table aip_desk_item
(
  SEQ_ID        NVARCHAR2(36) not null,
  DESK_ID       NVARCHAR2(36),
  folder_id     NVARCHAR2(50),
  SOURCE_TYPE   NVARCHAR2(50),
  SOURCE_ID     NVARCHAR2(36),
  LEFT          NVARCHAR2(10),
  TOP           NVARCHAR2(10),
  WIDTH         NVARCHAR2(10),
  HEIGHT        NVARCHAR2(10),
  TYPE          NVARCHAR2(20),
  NAME          NVARCHAR2(255),
  BELONG_TO     NVARCHAR2(36),
  BELONG_TYPE   NVARCHAR2(50),
  DISPLAY_ORDER NUMBER(11),
  IS_DELETED    NUMBER(11),
  e_code        NVARCHAR2(50) not null
)
;
comment on table aip_desk_item
  is '包括快捷方式，小挂件';
comment on column aip_desk_item.DESK_ID
  is '桌面ID';
comment on column aip_desk_item.folder_id
  is '文件夹id';
comment on column aip_desk_item.SOURCE_TYPE
  is '来源类型';
comment on column aip_desk_item.SOURCE_ID
  is '来源编号（如appfunction Id）';
comment on column aip_desk_item.LEFT
  is '快捷图标坐标';
comment on column aip_desk_item.TOP
  is '快捷图标坐标';
comment on column aip_desk_item.WIDTH
  is '宽';
comment on column aip_desk_item.HEIGHT
  is '高';
comment on column aip_desk_item.TYPE
  is '类型';
comment on column aip_desk_item.NAME
  is '未使用（可以作为别名使用）';
comment on column aip_desk_item.e_code
  is '企业编码';
alter table aip_desk_item
  add primary key (SEQ_ID);

