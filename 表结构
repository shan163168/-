prompt
prompt Creating table uam_user_field_operation
prompt =======================================
prompt
create table uam_user_field_operation
(
  id                NVARCHAR2(36) not null,
  code              NVARCHAR2(50) not null,
  name              NVARCHAR2(200) not null,
  description       NVARCHAR2(1000),
  default_fields    NVARCHAR2(300),
  use_scope         NUMBER(4) not null,
  additional_fields NVARCHAR2(200),
  support_fields    NVARCHAR2(300),
  created_date      DATE not null,
  created_by        NVARCHAR2(50) not null
)
;
comment on table uam_user_field_operation
  is '用户字段操作';
comment on column uam_user_field_operation.id
  is '字段操作id';
comment on column uam_user_field_operation.code
  is '操作code';
comment on column uam_user_field_operation.name
  is '操作名称';
comment on column uam_user_field_operation.description
  is '操作描述';
comment on column uam_user_field_operation.default_fields
  is '操作默认对应的字段（field1|field2|…）';
comment on column uam_user_field_operation.use_scope
  is '使用范围（0:服务端，1:服务端和终端）';
