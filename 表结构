prompt Creating table uam_user_blacklist
prompt =================================
prompt
create table uam_user_blacklist
(
  id                NVARCHAR2(36) not null,
  user_id           NVARCHAR2(36) not null,
  black_id          NVARCHAR2(36) not null,
  type_y              NUMBER(4) not null,
  remark            NVARCHAR2(100),
  is_deleted        NUMBER(4),
  created_by        NVARCHAR2(36),
  created_date      DATE,
  last_updated_by   NVARCHAR2(36),
  last_updated_date DATE,
  e_code            NVARCHAR2(50)
)
;
comment on table uam_user_blacklist
  is '用户黑名单关联表';
comment on column uam_user_blacklist.id
  is 'id';
comment on column uam_user_blacklist.user_id
  is '用户ID';
comment on column uam_user_blacklist.black_id
  is '黑名单ID';
comment on column uam_user_blacklist.type_y
  is '黑名单类型（默认 1：用户）';
comment on column uam_user_blacklist.remark
  is '备注';
comment on column uam_user_blacklist.is_deleted
  is '是否删除';
comment on column uam_user_blacklist.created_by
  is '创建者';
comment on column uam_user_blacklist.created_date
  is '创建时间';
comment on column uam_user_blacklist.last_updated_by
  is '最后更新者';
comment on column uam_user_blacklist.last_updated_date
  is '最后更新时间';
comment on column uam_user_blacklist.e_code
  is '企业eCode';

