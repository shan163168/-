prompt
create table tmp_org
(
  id         NVARCHAR2(200) not null,
  orgCode    NVARCHAR2(200),
  orgName    NVARCHAR2(200),
  sortName   NVARCHAR2(200),
  parentCode NVARCHAR2(200),
  parentName NVARCHAR2(200),
  isTemp     NUMBER(4),
  orgSort    NVARCHAR2(100),
  isDeleted  NUMBER(4),
  updateTime DATE,
  status     NUMBER(4)
)
;
comment on table tmp_org
  is '机构临时表';
comment on column tmp_org.id
  is '组织机构id';
