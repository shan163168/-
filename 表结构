create table pan_permission
(
  id         NVARCHAR2(50) not null,
  scope_id   NVARCHAR2(50) not null,
  ops        NUMBER(11) not null,
  merged_ops NUMBER(11) not null,
  is_inherit NVARCHAR2(1) not null,
  owner      NVARCHAR2(50) not null,
  immutable  NVARCHAR2(1) not null,
  updator_id NVARCHAR2(38),
  updated_at DATE
)
;
comment on table pan_permission
  is '权限表';
comment on column pan_permission.scope_id
  is '对应的资源 scope_id';
comment on column pan_permission.ops
  is '单独分配操作符值（可多个）';
comment on column pan_permission.merged_ops
  is '经过继承运算的操作符值，权限使用该字段进行判断';
comment on column pan_permission.is_inherit
  is '是否使用继承关系';
comment on column pan_permission.owner
  is '权限拥有者，例如：Uxxxx，Dxxxx';
comment on column pan_permission.immutable
  is '指示该授权记录是否不可更改';

