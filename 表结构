create table sso_remote_app_token
(
  USER_ID      NVARCHAR2(36) not null,
  TOKEN        NVARCHAR2(150),
  LAST_UPDATED DATE
)
;
COMMENT ON COLUMN sso_remote_app_token.USER_ID IS '用户ID';
COMMENT ON COLUMN sso_remote_app_token.TOKEN IS '令牌';
COMMENT ON COLUMN sso_remote_app_token.LAST_UPDATED IS '最后更新时间';

