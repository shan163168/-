create table wf_task
(
  id_                    NVARCHAR2(36) not null,
  app_code               NVARCHAR2(36),
  revision_              NUMBER(11) not null,
  seq_                   NUMBER(11),
  batch_                 NUMBER(11),
  proc_def_id            NVARCHAR2(36) not null,
  proc_inst_id           NVARCHAR2(36) not null,
  root_inst_id           NVARCHAR2(36) not null,
  execution_id           NVARCHAR2(36),
  act_inst_id            NVARCHAR2(36),
  parent_id              NVARCHAR2(36),
  deliver_task_id        NVARCHAR2(36),
  pre_task_id            NVARCHAR2(36),
  form_key               NVARCHAR2(150),
  business_id            NVARCHAR2(36),
  business_data          NCLOB,
  type_                  NUMBER(6) not null,
  title_                 NVARCHAR2(300) not null,
  priority_              NUMBER(6) not null,
  act_name               NVARCHAR2(150) not null,
  act_title              NVARCHAR2(300),
  creator_id             NVARCHAR2(36),
  creator_name           NVARCHAR2(150),
  assignee_id            NVARCHAR2(36),
  assignee_name          NVARCHAR2(150),
  assignee_dept_id       NVARCHAR2(36),
  assignee_dept_name     NVARCHAR2(150),
  assignee_identity_id   NVARCHAR2(36),
  assignee_identity_name NVARCHAR2(150),
  agent_id               NVARCHAR2(36),
  agent_name             NVARCHAR2(150),
  agent_dept_id          NVARCHAR2(36),
  agent_dept_name        NVARCHAR2(150),
  agent_identity_id      NVARCHAR2(36),
  agent_identity_name    NVARCHAR2(150),
  expire_time            DATE,
  start_time             DATE,
  claim_time             DATE,
  read_time              DATE,
  end_time               DATE,
  duration_              NUMBER(20),
  status_                NUMBER(6) not null,
  end_reason             NUMBER(6),
  end_comment            NVARCHAR2(500),
  user_input             NCLOB,
  user_comment           NCLOB,
  description_           NCLOB,
  deleted_               NUMBER(6) not null,
  deleted_time           DATE,
  deleted_comment        NVARCHAR2(500),
  terminal               NVARCHAR2(50)
)
;
