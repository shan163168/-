(
  org_id   NVARCHAR2(36) not null,
  org_name NVARCHAR2(50)
)
;

COMMENT ON COLUMN app_rank_blacklist.org_id IS '组织ID';
COMMENT ON COLUMN app_rank_blacklist.org_name IS '组织名称';

alter table app_rank_blacklist
  add primary key (org_id);

