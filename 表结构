prompt Creating table pan_sys_permission
prompt =================================
prompt
create table pan_sys_permission
(
  ID                NVARCHAR2(36) not null,
  PARENT            NVARCHAR2(36),
  CODE              NVARCHAR2(100) not null,
  NAME              NVARCHAR2(255) not null,
  URL               NVARCHAR2(300),
  TYPE              NVARCHAR2(20) not null,
  ELEMENT_ID        NVARCHAR2(50),
  ELEMENT_BEHAVIOUR NVARCHAR2(30),
  ORDER_Y             NUMBER(11) not null,
  DESCRIPTION       NVARCHAR2(1000),
  CREATED_BY        NVARCHAR2(36) not null,
  CREATED_DATE      DATE not null,
  LAST_UPDATED_BY   NVARCHAR2(36) not null,
  LAST_UPDATED_DATE DATE not null,
  THUMBNAIL         NVARCHAR2(300),
  ICON              NVARCHAR2(300),
  ICON_LARGE        NVARCHAR2(300),
  ICON_MIDDLE       NVARCHAR2(300),
  ICON_SMALL        NVARCHAR2(300),
  SUBTYPE           NVARCHAR2(20),
  STATUS            NUMBER(11),
  WIDTH             NVARCHAR2(20),
  HEIGHT            NVARCHAR2(20),
  CONFIGURL         NVARCHAR2(300),
  WIDGET_TYPE       NVARCHAR2(20),
  RENDERTYPE        NVARCHAR2(20),
  APP_ID            NVARCHAR2(36),
  IS_REFERENCE      NUMBER(4),
  REFERENCE         NVARCHAR2(50),
  e_code            NVARCHAR2(50) not null
)
;
comment on table pan_sys_permission
  is '操作权限表';
comment on column pan_sys_permission.ID
  is '操作标识';
comment on column pan_sys_permission.PARENT
  is '父操作标识';
