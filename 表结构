prompt
prompt Creating table uam_user_account
prompt ===============================
prompt
create table uam_user_account
(
  id                NVARCHAR2(36) not null,
  user_id           NVARCHAR2(36) not null,
  login_id          NVARCHAR2(50) not null,
  type_y              NUMBER(6) not null,
  status            NUMBER(6) not null,
  e_code            NVARCHAR2(45) not null,
  created_date      DATE not null,
  created_by        NVARCHAR2(36) not null,
  last_updated_by   NVARCHAR2(36) not null,
  last_updated_date DATE not null,
  source            NVARCHAR2(10),
  source_id         NVARCHAR2(38)
)
;
comment on table uam_user_account
  is '用户登录帐号';
comment on column uam_user_account.user_id
  is '用户Id';
comment on column uam_user_account.login_id
  is '登录帐号';
comment on column uam_user_account.type_y
  is '登录帐号类型，0：帐号，1：手机号';
comment on column uam_user_account.status
  is '状态，0：禁用，1:正常';
comment on column uam_user_account.e_code
  is '所属企业';
comment on column uam_user_account.source
  is '数据来源:AD、Link、IAM';
comment on column uam_user_account.source_id
  is '数据源ID';

