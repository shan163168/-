prompt
create table uam_external_org_type
(
  TYPE_ID           NVARCHAR2(50) not null,
  NAME              NVARCHAR2(100),
  CODE              NVARCHAR2(50),
  MEMO              NVARCHAR2(500),
  CREATED_DATE      DATE,
  LAST_UPDATED_DATE DATE,
  CREATED_BY        NVARCHAR2(50),
  LAST_UPDATED_BY   NVARCHAR2(50),
  e_code            NVARCHAR2(50) not null
)
;
comment on column uam_external_org_type.TYPE_ID
  is '分类ID';
comment on column uam_external_org_type.NAME
  is '分类名称';
comment on column uam_external_org_type.CODE
  is '分类代码';
comment on column uam_external_org_type.MEMO
  is '备注';
comment on column uam_external_org_type.e_code
  is '企业编码';

