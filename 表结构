prompt Creating table meta_relation
prompt ============================
prompt
create table meta_relation
(
  id                 NVARCHAR2(36) not null,
  metabase           NVARCHAR2(38) not null,
  source_entity      NVARCHAR2(38) not null,
  target_entity      NVARCHAR2(38) not null,
  join_entity        NVARCHAR2(38),
  display_name       NVARCHAR2(150),
  name               NVARCHAR2(150) not null,
  type               NVARCHAR2(38) not null,
  association        NVARCHAR2(38),
  is_optional        NVARCHAR2(1) not null,
  is_column_nullable NVARCHAR2(1),
  is_update          NVARCHAR2(1) not null,
  on_delete          NVARCHAR2(36),
  is_system          NVARCHAR2(1),
  is_display         NVARCHAR2(1),
  display_for        NVARCHAR2(150),
  display_order      NUMBER(11),
  summary            NVARCHAR2(1000),
  description        NCLOB,
  creation_time      DATE not null,
  last_modified      DATE not null,
  Is_Abstract        NVARCHAR2(1),
  join_fields        NVARCHAR2(500),
  is_copy            NVARCHAR2(1)
)
;
