prompt
prompt Creating table app_ext
prompt ======================
prompt
create table app_ext
(
  PERMISSION_ID       NVARCHAR2(50) not null,
  CONFIG_URL          NVARCHAR2(500),
  LOGO                NVARCHAR2(500),
  VERSION             NVARCHAR2(20),
  STATUS              NVARCHAR2(20),
  VISIBLE_ALL         NVARCHAR2(20),
  APP_FUNCTION_REMOTE NVARCHAR2(500),
  APP_WIDGET_REMOTE   NVARCHAR2(500),
  APP_STORE_CODE      NVARCHAR2(50)
)
;
comment on column app_ext.PERMISSION_ID
  is 'APP_FUNCTION 扩展';
comment on column app_ext.CONFIG_URL
  is 'APP_FUNCTION 扩展';
comment on column app_ext.LOGO
  is 'APP_扩展';
comment on column app_ext.VERSION
  is 'APP_扩展';
comment on column app_ext.STATUS
  is 'APP_扩展';
alter table app_ext
  add primary key (PERMISSION_ID);

prompt
prompt Creating table app_feedback_replay
prompt ==================================
prompt
create table app_feedback_replay
(
  replay_id      NVARCHAR2(38) not null,
  report_id      NVARCHAR2(38),
  from_user_id   NVARCHAR2(38),
  to_user_id     NVARCHAR2(38),
  replay_content NVARCHAR2(200),
  created_time   DATE
)
;
comment on table app_feedback_replay
  is '反馈回复表';
comment on column app_feedback_replay.replay_id
  is '编号';
comment on column app_feedback_replay.report_id
  is '反馈编号';
comment on column app_feedback_replay.from_user_id
  is '反馈人id';
comment on column app_feedback_replay.to_user_id
  is '接收人id';
comment on column app_feedback_replay.replay_content
  is '反馈内容';
comment on column app_feedback_replay.created_time
  is '反馈时间';
alter table app_feedback_replay
  add primary key (replay_id);

prompt
prompt Creating table app_feedback_report
prompt ==================================
prompt
create table app_feedback_report
(
  report_id     NVARCHAR2(38) not null,
  category_name NVARCHAR2(20),
  module_name   NVARCHAR2(20),
  report_type   NVARCHAR2(20),
  content       NVARCHAR2(200),
  status        NUMBER(11),
  created_by    NVARCHAR2(38),
  created_time  DATE
)
;
comment on table app_feedback_report
  is '问题反馈表';
comment on column app_feedback_report.report_id
  is '反馈编号';
comment on column app_feedback_report.category_name
  is '分类编号';
comment on column app_feedback_report.module_name
  is '模块编号';
comment on column app_feedback_report.report_type
  is '反馈类别:1 意见建议 2 问题反馈';
comment on column app_feedback_report.content
  is '反馈内容';
comment on column app_feedback_report.status
  is '反馈状态:0 待处理 1 已处理';
comment on column app_feedback_report.created_by
  is '反馈人编号';
comment on column app_feedback_report.created_time
  is '反馈提交时间';
alter table app_feedback_report
  add primary key (report_id);

