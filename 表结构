prompt Creating table uam_external_organization
prompt ========================================
prompt
create table uam_external_organization
(
  ORG_ID            NVARCHAR2(50) not null,
  ORG_NUM           NVARCHAR2(128),
  NAME              NVARCHAR2(64) not null,
  FULL_NAME         NVARCHAR2(255),
  PARENT_ID         NVARCHAR2(50),
  CODE              NVARCHAR2(1000),
  level_y             NUMBER(11),
  TYPE_Y              NVARCHAR2(50),
  STATUS            NUMBER(6) not null,
  ORDER_NO          NUMBER(11),
  DESCRIPTION       NVARCHAR2(1000),
  ICODE             NUMBER(11),
  CREATED_DATE      DATE,
  LAST_UPDATED_DATE DATE,
  createdBy         NVARCHAR2(500),
  lastUpdatedBy     NVARCHAR2(500),
  e_code            NVARCHAR2(50) not null
)
;
comment on table uam_external_organization
  is '组织架构的部门';
comment on column uam_external_organization.ORG_ID
  is '组织ID';
comment on column uam_external_organization.ORG_NUM
  is '组织编号';
