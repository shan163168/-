  display_order     NUMBER(11) not null,
  field_type        NVARCHAR2(38),
  options           NCLOB,
  field_type_params NVARCHAR2(1000),
  description       NCLOB,
  length            NUMBER(11),
  is_deleted        NVARCHAR2(1) not null,
  created_date      DATE not null,
  created_by        NVARCHAR2(50) not null,
  e_code            NVARCHAR2(50) not null
)
;
