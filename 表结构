prompt Creating table temp_user
prompt ========================
prompt
create table temp_user
(
  icode           NUMBER(11) not null,
  idCard          NVARCHAR2(200),
  name            NVARCHAR2(200),
  sex             NVARCHAR2(10),
  birth           NVARCHAR2(100),
  policeNumber    NVARCHAR2(500) not null,
  policeCategory  NVARCHAR2(200),
  policeBusiness  NVARCHAR2(200),
  nation          NVARCHAR2(10),
  address         NVARCHAR2(500),
  mobileWork      NVARCHAR2(100),
  orgCode         NVARCHAR2(200),
  sort            NVARCHAR2(10),
  rank            NVARCHAR2(200),
  title           NVARCHAR2(200),
  manType         NVARCHAR2(200),
  postType        NVARCHAR2(200),
  jobType         NVARCHAR2(200),
  auxiliaryType   NVARCHAR2(200),
  userStatus      NVARCHAR2(200),
  type_y            NUMBER(11),
  deleted         NVARCHAR2(10),
  updateTime      DATE,
  status          NUMBER(11),
  initialPassword NVARCHAR2(100),
  smsStatus       NUMBER(11)
)
;
comment on column temp_user.sex
  is '性别 1：男 2：女';
comment on column temp_user.policeNumber
  is '警号';
comment on column temp_user.policeCategory
  is '警种';
comment on column temp_user.policeBusiness
  is '业务域';
comment on column temp_user.rank
  is '警衔';
comment on column temp_user.title
  is '职级';
comment on column temp_user.manType
  is '人员身份类型10：民警、15：现役特警、20：辅警、25：外部党政人员、30：在编文员、99999：其他工勤人员';
comment on column temp_user.postType
  is '职务';
comment on column temp_user.jobType
  is '在职类型 11：在职、12：借调、13：挂职、15：退休、16：离职、18：其他';
comment on column temp_user.auxiliaryType
  is '辅警类型 10：合同工、20：聘用、30：其它';
comment on column temp_user.userStatus
  is '人员状态 1：在职、0：离职';
comment on column temp_user.type_y
  is '1 民警;2 辅警';
comment on column temp_user.updateTime
  is '更新时间';
comment on column temp_user.status
  is '0 未处理;1 已处理';
comment on column temp_user.initialPassword
  is '初始密码';
comment on column temp_user.smsStatus
  is '0 未发送；1 失败1次；2 失败2次；3 失败3次；4 失败4次；5 失败过多；8 成功；9 旧用户不发送';

