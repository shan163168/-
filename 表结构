prompt Creating table aip_desk
prompt =======================
prompt
create table aip_desk
(
  SEQ_ID         NVARCHAR2(36) not null,
  NAME           NVARCHAR2(255),
  CODE           NVARCHAR2(50),
  DESCRIPTION    NVARCHAR2(1000),
  PORTAL_ID      NVARCHAR2(36),
  DISPLAY_ORDER  NUMBER(11),
  BELONG_TO      NVARCHAR2(50),
  BELONG_TYPE    NVARCHAR2(20),
  BELONG_DESK_ID NVARCHAR2(36),
  e_code         NVARCHAR2(50) not null
)
;
comment on column aip_desk.NAME
  is '桌面名称';
comment on column aip_desk.CODE
  is '桌面代码';
comment on column aip_desk.DESCRIPTION
  is '描述';
comment on column aip_desk.DISPLAY_ORDER
  is '显示顺序';
comment on column aip_desk.BELONG_TO
  is '所属名称（如用户ID）';
comment on column aip_desk.BELONG_TYPE
  is '所属类型（如user，用户）';
comment on column aip_desk.BELONG_DESK_ID
  is '所属桌面ID，继承默认桌面的一些信息';
comment on column aip_desk.e_code
  is '企业编码';
alter table aip_desk
  add primary key (SEQ_ID);



