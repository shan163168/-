prompt Creating table am_app_version
prompt =============================
prompt
create table am_app_version
(
  VERSION_ID             NVARCHAR2(36) not null,
  SUB_APP_ID             NVARCHAR2(36),
  VERSION_NAME           NVARCHAR2(255),
  VERSION_NUM            NUMBER(11),
  DESCRIPTION            NVARCHAR2(1000),
  IS_CLOUD_DEPLOY        NUMBER(4),
  INSTALL_PACKAGE_URL    NVARCHAR2(300),
  INSTALL_VARIABLE       NCLOB,
  APP_WEBURL             NVARCHAR2(300),
  HELP_URL               NVARCHAR2(300),
  ADMIN_URL              NVARCHAR2(300),
  SETTING_URL            NVARCHAR2(300),
  DEPLOYTEMPLATE         NCLOB,
  IS_CLOUD_COMPILE       NUMBER(4),
  IS_SILENT_INSTALLATION NUMBER(4),
  is_check_update        NUMBER(4),
  custom_app_code        NVARCHAR2(50),
  APP_FRAMEWORK_TYPE     NVARCHAR2(50),
  AUTH_TYPE              NVARCHAR2(20),
  SECURITY_TYPE          NUMBER(6),
  APP_ACTION             NVARCHAR2(500),
  DOWNLOAD_URL           NVARCHAR2(300),
  DOWNLOAD_NUM           NUMBER(11),
  IS_PUBLISH             NUMBER(4),
  is_trial               NUMBER(4),
  CREATED_BY             NVARCHAR2(36),
  created_client         NVARCHAR2(50),
  created_date           DATE,
  LAST_UPDATED_BY        NVARCHAR2(36),
  last_updated_client    NVARCHAR2(50),
  last_updated_date      DATE,
  external_id            NVARCHAR2(36),
  SIZE_Y                   NUMBER(20),
  NUMBER_URL             NVARCHAR2(300),
  WIN_STATUS             NUMBER(11),
  IS_DEFAULT_APP         NUMBER(4),
  extend_infos           NCLOB,
  is_force_update        NUMBER(4),
  e_code                 NVARCHAR2(50) not null,
  source_id              NVARCHAR2(36)
)
;
comment on column am_app_version.VERSION_NUM
  is '版本号';
comment on column am_app_version.is_check_update
  is '是否检查版本升级：选择“是”则检查，选择“否”不检查，默认为“是”';
comment on column am_app_version.custom_app_code
  is '程序包编码：用户手工填写，选填项，用于关联应用市场的安装包';
comment on column am_app_version.APP_FRAMEWORK_TYPE
  is '对终端应用：
              原生应用、bingoTouch应用.
            对web应用：
              元数据应用...';
comment on column am_app_version.SECURITY_TYPE
  is '安全类型 1:互联网应用,2:移动专网应用,3:内网应用';
comment on column am_app_version.is_trial
  is '是否是试用版';
comment on column am_app_version.created_client
  is '创建的client';
comment on column am_app_version.last_updated_client
  is '最后更新的client';
comment on column am_app_version.external_id
  is '外部数据源ID';
comment on column am_app_version.extend_infos
  is '应用扩展信息';
comment on column am_app_version.is_force_update
  is '是否强制更新';
comment on column am_app_version.e_code
  is '企业编码';
comment on column am_app_version.source_id
  is '源ID';
alter table am_app_version
  add primary key (VERSION_ID);

