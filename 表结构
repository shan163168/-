create table wf_execution
(
  id_               NVARCHAR2(36) not null,
  app_code          NVARCHAR2(36),
  revision_         NUMBER(11) not null,
  seq_              NUMBER(11),
  path_             NVARCHAR2(50),
  root_inst_id      NVARCHAR2(36),
  proc_def_id       NVARCHAR2(36) not null,
  proc_inst_id      NVARCHAR2(36) not null,
  caller_inst_id    NVARCHAR2(36),
  caller_exec_id    NVARCHAR2(36),
  parent_id         NVARCHAR2(36),
  parent_act_name   NVARCHAR2(150),
  parent_act_title  NVARCHAR2(300),
  parent_act_path   NVARCHAR2(50),
  start_act_name    NVARCHAR2(150),
  start_act_title   NVARCHAR2(300),
  income_trans_name NVARCHAR2(150),
  curr_act_inst_id  NVARCHAR2(36),
  last_task_id      NVARCHAR2(36),
  curr_act_path     NVARCHAR2(50),
  curr_act_name     NVARCHAR2(150),
  curr_act_title    NVARCHAR2(300),
  curr_act_started  NUMBER(6) not null,
  end_act_name      NVARCHAR2(150),
  end_act_title     NVARCHAR2(300),
  parallel_         NUMBER(6) not null,
  parallel_type     NUMBER(6) not null,
  start_time        DATE,
  end_time          DATE,
  duration_         NUMBER(20),
  status_           NUMBER(6) not null,
  end_reason        NUMBER(6),
  end_comment       NVARCHAR2(500),
  deleted_          NUMBER(6) not null,
  deleted_time      DATE,
  deleted_comment   NVARCHAR2(500),
  child_seq         NUMBER(6),
  act_seq           NUMBER(11) not null
)
