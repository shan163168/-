prompt
create table sm_service_no
(
  account_id            NVARCHAR2(50) not null,
  name                  NVARCHAR2(255),
  CODE                  NVARCHAR2(50),
  ICON                  NVARCHAR2(300),
  description           NCLOB,
  category              NVARCHAR2(36),
  NAME_PY               NVARCHAR2(500),
  NAME_PINYIN           NVARCHAR2(100),
  IS_ENABLED            NUMBER(11),
  IS_STICKY             NUMBER(4),
  org_id                NVARCHAR2(50),
  CREATED_BY            NVARCHAR2(36),
  CREATED_DATE          DATE,
  LAST_UPDATED_BY       NVARCHAR2(36),
  LAST_UPDATED_DATE     DATE,
  IS_AUDIT              NUMBER(11) not null,
  IS_DELETED            NUMBER(4) not null,
  ORDER_NO              NUMBER(11) not null,
  IS_NOTIFYCATION       NUMBER(4),
  IS_TERMINAL_SHOW      NUMBER(6),
  SECRET_KEY            NVARCHAR2(50),
  TOKEN                 NVARCHAR2(50),
  INVOKE_TYPE           NUMBER(6),
  service_url           NVARCHAR2(300),
  is_transit            NUMBER(4),
  msg_type              NUMBER(6),
  app_code              NVARCHAR2(100),
  MSG_RECEIPT_TYPE      NUMBER(4),
  IS_SYSTEM             NUMBER(4),
  IS_RECEIVE_MSG        NUMBER(4),
  receive_msg_set       NVARCHAR2(50),
  is_confine_fans       NUMBER(4),
  is_allow_reply        NUMBER(4),
  E_Code                NVARCHAR2(50),
  IS_FORCE_RECEIVE      NUMBER(4),
  DEFAULT_RECEIVE_SCOPE NUMBER(6) not null,
  OWNER                 NVARCHAR2(38),
  grade                 NUMBER(4),
  region_id             NVARCHAR2(36)
)
;
