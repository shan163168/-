  Shahex             NVARCHAR2(50),
  server_path        NVARCHAR2(100),
  is_deleted         NUMBER(4),
  source_code        NVARCHAR2(10),
  source_file_prikey NVARCHAR2(100),
  source_file_pubkey NVARCHAR2(100),
  module             NVARCHAR2(20),
  disk_code          NVARCHAR2(50),
  security           NUMBER(4),
  file_encrypted_key NVARCHAR2(150),
  status             NUMBER(4),
  created_by         NVARCHAR2(50),
  created_date       DATE,
