prompt Creating table sys_dictionaries
prompt ===============================
prompt
create table sys_dictionaries
(
  ID          NVARCHAR2(38) not null,
  CODE        NVARCHAR2(50) not null,
  NAME        NVARCHAR2(150),
  DESCRIPTION NVARCHAR2(1000),
  IS_DELETE   NUMBER(6) not null,
  e_code      NVARCHAR2(50) not null,
  type_y        NUMBER(6),
  is_system   NUMBER(4),
  data_type   NUMBER(4),
  remark      NVARCHAR2(100),
  order_by    NUMBER(4),
  category    NVARCHAR2(40),
  use_scope   NUMBER(4)
)
;
comment on table sys_dictionaries
  is '数据字典表';
comment on column sys_dictionaries.ID
  is '字典标识';
comment on column sys_dictionaries.CODE
  is '字典编号';
comment on column sys_dictionaries.NAME
  is '字典名称';
comment on column sys_dictionaries.DESCRIPTION
  is '字典描述';
comment on column sys_dictionaries.IS_DELETE
  is '是否已删除';
comment on column sys_dictionaries.e_code
  is '企业编码';
comment on column sys_dictionaries.type_y
  is '参数类型 0:企业管理的参数，1平台管理员管理的企业参数';
comment on column sys_dictionaries.is_system
  is '1：是，0：否，系统参数不提供用户设置';
comment on column sys_dictionaries.data_type
  is '1:数字，2:字符串，3:on或off，4:1或0，5:true或false';
comment on column sys_dictionaries.remark
  is '备注：description字段其实是值来的';
comment on column sys_dictionaries.order_by
  is '排序';
comment on column sys_dictionaries.category
  is '应用模块';
comment on column sys_dictionaries.use_scope
  is '使用范围(0:服务端(不对终端提供)，1:服务端和终端)';

