prompt
create table uam_user_1207
(
  USER_ID           NVARCHAR2(100) not null,
  user_num          NVARCHAR2(50),
  NAME              NVARCHAR2(50) not null,
  FULL_NAME         NVARCHAR2(255),
  alias             NVARCHAR2(255),
  LOGIN_ID          NVARCHAR2(50),
  PASSWORD          NVARCHAR2(255),
  ORG_ID            NVARCHAR2(36),
  DOMAIN            NVARCHAR2(50),
  TYPE_Y              NUMBER(6),
  STATUS            NUMBER(11),
  SEX               NUMBER(6),
  RANK              NUMBER(11),
  EMAIL             NVARCHAR2(50),
  MOBILE            NVARCHAR2(60),
  IM                NVARCHAR2(50),
  TELEPHONE         NVARCHAR2(255),
  BIRTHDAY          DATE,
  PICTURE           NVARCHAR2(300),
  ORDER_NO          NUMBER(11) not null,
  DESCRIPTION       NVARCHAR2(1000),
  control_client    NVARCHAR2(50),
  is_hidden         NUMBER(4),
  is_active         NUMBER(4),
  identity_num      NVARCHAR2(20),
  extend_fields     NCLOB,
  work_status       NVARCHAR2(20),
  CREATED_DATE      DATE,
  LAST_UPDATED_DATE DATE,
  CREATED_BY        NVARCHAR2(100),
  LAST_UPDATED_BY   NVARCHAR2(100),
  DISK_QUOTA        NUMBER(11),
  HOME_TELEPHONE    NVARCHAR2(20),
  MOBILE2           NVARCHAR2(20),
  MOBILE3           NVARCHAR2(20),
  name_pinyin       NVARCHAR2(250),
  NAME_PY           NVARCHAR2(50),
  POST_DESCRIPTION  NVARCHAR2(255),
  HAS_INIT_PASSWORD NUMBER(11),
  SYSTEM_MAP_ID     NVARCHAR2(50),
  LINK_SETTING      NCLOB,
  IS_DELETED        NUMBER(4),
  TOKEN             NVARCHAR2(100),
  APP_PASSWORD      NVARCHAR2(100),
  S_CODE            NVARCHAR2(50),
  S_ID              NVARCHAR2(36),
  ref_id            NVARCHAR2(100),
  E_Code            NVARCHAR2(100),
  source            NVARCHAR2(10),
  is_Recommended    NUMBER(11),
  Secret_level      NUMBER(6),
  labor             NVARCHAR2(50),
  Native_Place      NVARCHAR2(50),
  Political         NVARCHAR2(50),
  nationality       NVARCHAR2(50),
  Positional_Title  NVARCHAR2(50),
  degree            NVARCHAR2(50),
  major             NVARCHAR2(50),
  address           NVARCHAR2(250),
  fax               NVARCHAR2(50),
  source_id         NVARCHAR2(38),
  actived_date      DATE
)
;
