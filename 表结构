prompt
prompt Creating table ad_advert
prompt ========================
prompt
create table ad_advert
(
  id                NVARCHAR2(36) not null,
  name              NVARCHAR2(500) not null,
  advertising       NVARCHAR2(36),
  type              NUMBER(4),
  content           NCLOB,
  org_id            NVARCHAR2(50),
  priority          NUMBER(11),
  link              NVARCHAR2(1000),
  duration          NUMBER(11) not null,
  created_by        NVARCHAR2(36),
  created_date      DATE,
  last_updated_by   NVARCHAR2(36),
  last_updated_date DATE,
  status            NUMBER(4),
  e_code            NVARCHAR2(50) not null
)
;
comment on column ad_advert.name
  is '广告名';
comment on column ad_advert.advertising
  is '广告位';
comment on column ad_advert.type
  is '广告类型';
comment on column ad_advert.content
  is '广告内容';
comment on column ad_advert.org_id
  is '所属部门';
comment on column ad_advert.priority
  is '优先级';
comment on column ad_advert.link
  is '广告链接';
comment on column ad_advert.duration
  is '页面持续时间';
comment on column ad_advert.status
  is '状态';
comment on column ad_advert.e_code
  is '企业编码';
alter table ad_advert
  add primary key (id);

