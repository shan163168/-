  DESCRIPTION            NVARCHAR2(1000),
  IMAGES                 NCLOB,
  CHECKIN_RECEIVERS      NCLOB,
  MESSAGE_RECEIVERS      NCLOB,
  IS_PUBLIC              NUMBER(4) not null,
  PUBLISH_TO_BLOG        NUMBER(4),
  BLOG_ID                NVARCHAR2(36),
