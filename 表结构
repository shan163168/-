prompt ===========================
prompt
create table uam_favorite
(
  id                  NVARCHAR2(36) not null,
  source_id           NVARCHAR2(36) not null,
  source_type         NUMBER(11) not null,
  source_name         NVARCHAR2(50) not null,
  content_type        NUMBER(11) not null,
  content             NCLOB not null,
  created_by          NVARCHAR2(36),
  created_date        DATE,
  is_deleted          NUMBER(4),
  keyword             NCLOB,
  item_id             NVARCHAR2(50),
  source_display_name NVARCHAR2(50)
)
;
comment on column uam_favorite.source_id
  is '来源ID';
comment on column uam_favorite.source_type
  is '来源类型';
comment on column uam_favorite.source_name
  is '来源名称';
comment on column uam_favorite.content_type
  is '收藏类型';
comment on column uam_favorite.content
  is '收藏内容';
