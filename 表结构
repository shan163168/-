prompt Creating table uam_group_robot_source
prompt =====================================
prompt
create table uam_group_robot_source
(
  id                NVARCHAR2(36) not null,
  name              NVARCHAR2(200) not null,
  icon              NVARCHAR2(300),
  type_y              NUMBER(6),
  code              NVARCHAR2(50) not null,
  introduction      NVARCHAR2(500) not null,
  preview_image     NVARCHAR2(300) not null,
  website           NVARCHAR2(100),
  is_deleted        NVARCHAR2(1) not null,
  created_by        NVARCHAR2(36),
  created_date      DATE,
  last_updated_by   NVARCHAR2(36),
  last_updated_date DATE,
  e_code            NVARCHAR2(50)
)
;
comment on table uam_group_robot_source
  is '机器人来源表';
comment on column uam_group_robot_source.id
  is '类型ID';
comment on column uam_group_robot_source.name
