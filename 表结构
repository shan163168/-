  id             NVARCHAR2(38) not null,
  principal_id   NVARCHAR2(38),
  principal_type NVARCHAR2(1),
  resource_id    NVARCHAR2(38),
  scope          NVARCHAR2(1000),
  created_at     DATE not null,
  created_by     NVARCHAR2(38),
  updated_at     DATE not null,
  updated_by     NVARCHAR2(38),
  audit_state    NUMBER(11),
  audit_by       NVARCHAR2(38),
  audit_at       DATE
)
;
COMMENT ON COLUMN oauth2_permission.id IS '权限ID';
COMMENT ON COLUMN oauth2_permission.principal_id IS '主体ID';
COMMENT ON COLUMN oauth2_permission.principal_type IS '主体类型';
COMMENT ON COLUMN oauth2_permission.resource_id IS '资源ID';
COMMENT ON COLUMN oauth2_permission.scope IS '权限范围';
COMMENT ON COLUMN oauth2_permission.created_at IS '创建时间';
COMMENT ON COLUMN oauth2_permission.created_by IS '创建者';
COMMENT ON COLUMN oauth2_permission.updated_at IS '更新时间';
COMMENT ON COLUMN oauth2_permission.updated_by IS '更新者';
COMMENT ON COLUMN oauth2_permission.audit_state IS '审核状态';
COMMENT ON COLUMN oauth2_permission.audit_by IS '审核者';
COMMENT ON COLUMN oauth2_permission.audit_at IS '审核时间';

