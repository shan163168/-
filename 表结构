prompt Creating table mkt_app
prompt ======================
prompt
create table mkt_app
(
  APP_ID                 NVARCHAR2(36) not null,
  APP_CATEGORY_ID        NVARCHAR2(36),
  CURRENT_VERSION_ID     NVARCHAR2(36),
  APP_CODE               NVARCHAR2(50),
  APP_NAME               NVARCHAR2(255),
  APP_DESCRIPTION        NCLOB,
  IS_ENABLED             NUMBER(4),
  IS_SYSTEM              NUMBER(4),
  IS_SILENT_INSTALLATION NUMBER(4),
  APP_TYPE               NUMBER(6),
  SUPPORT_PLAT           NUMBER(6),
  APP_ACTION             NCLOB,
  APP_level              NUMBER(11),
  APP_URL                NVARCHAR2(300),
  AUTH_TYPE              NUMBER(6),
  DEVELOPER              NVARCHAR2(255),
  DEVELOP_ORG            NVARCHAR2(255),
  DOWNLOAD_NUM           NUMBER(11),
  CREATED_BY             NVARCHAR2(36),
  CREATED_DATE           DATE,
  LAST_UPDATED_BY        NVARCHAR2(36),
  LAST_UPDATED_DATE      DATE,
  e_code                 NVARCHAR2(50) not null
)
;
comment on column mkt_app.IS_ENABLED
  is '0：不可用
            1：可用';
comment on column mkt_app.APP_TYPE
  is '1：移动原生应用
            2：移动BingoTouch应用
            3：标准Web应用';
comment on column mkt_app.SUPPORT_PLAT
  is '1：Both
            2：Android
            3 ：iOS
            4 ：iPhone
            5 ：iPad
            
            ';
comment on column mkt_app.APP_URL
  is '对于web应用，这是应用的入口地址
            对于终端应用，这是应用的下载地址';
comment on column mkt_app.AUTH_TYPE
  is '如果启用了验证，则该应用只能通过MobileOne启动。
            使用MobileOne验证服务的应用，要遵守一定的技术规范。
            1：不验证
            2：单点登录验证
            3：系统验证 
            4： 短信验证
            5：应用专属密码验证';
comment on column mkt_app.e_code
  is '企业编码';
alter table mkt_app
  add primary key (APP_ID);

