prompt Creating table uam_user_field
prompt =============================
prompt
create table uam_user_field
(
  id                NVARCHAR2(36) not null,
  name              NVARCHAR2(200) not null,
  display_name      NVARCHAR2(500) not null,
  display_order     NUMBER(11) not null,
  field_type        NVARCHAR2(38),
  options           NCLOB,
  field_type_params NVARCHAR2(1000),
  description       NCLOB,
  length            NUMBER(11),
  is_self           NVARCHAR2(1) not null,
  created_date      DATE not null,
  created_by        NVARCHAR2(50) not null
)
;
comment on table uam_user_field
  is '用户字段配置';
