prompt Creating table wf_variable
prompt ==========================
prompt
create table wf_variable
(
  id_          NVARCHAR2(36) not null,
  app_code     NVARCHAR2(36),
  revision_    NUMBER(11) not null,
  proc_inst_id NVARCHAR2(36) not null,
  execution_id NVARCHAR2(36),
  act_name     NVARCHAR2(150),
  task_id      NVARCHAR2(36),
  type_        NUMBER(6) not null,
  clazz_       NVARCHAR2(350),
  name_        NVARCHAR2(150) not null,
  value_       NCLOB
)
;
comment on table wf_variable is '工作流变量表';

comment on column wf_variable.id_ is '主键ID';
comment on column wf_variable.app_code is '应用程序代码';
comment on column wf_variable.revision_ is '版本号';
comment on column wf_variable.proc_inst_id is '流程实例ID';
comment on column wf_variable.execution_id is '执行ID';
comment on column wf_variable.act_name is '活动名称';
comment on column wf_variable.task_id is '任务ID';
comment on column wf_variable.type_ is '类型';
comment on column wf_variable.clazz_ is '类';
comment on column wf_variable.name_ is '名称';
