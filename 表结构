prompt Creating table sm_service
prompt =========================
prompt
create table sm_service
(
  ID                  NVARCHAR2(36) not null,
  CODE                NVARCHAR2(50),
  NAME                NVARCHAR2(255),
  version_num         NUMBER(11),
  CATEGORY            NVARCHAR2(36),
  ORDER_NO            NUMBER(11) not null,
  TYPE_Y                NUMBER(4),
  RECOMMEND           NUMBER(11),
  USED_COUNT          NUMBER(11),
  FAVORITE_COUNT      NUMBER(11),
  ICON                NVARCHAR2(300),
  ICON2               NVARCHAR2(300),
  DESCRIPTION         NCLOB,
  STATUS              NUMBER(11),
  PARAMS              NCLOB,
  TERMINAL_TYPE       NUMBER(11),
  ACTION_PARAMS       NCLOB,
  IS_TRIAL            NUMBER(4),
  is_hidden           NUMBER(4),
  AUTH_TYPE           NUMBER(4),
  open_mode           NUMBER(4),
  app_id              NVARCHAR2(36),
  app_scheme_id       NVARCHAR2(38),
  is_support_params   NUMBER(4),
  transfer_level      NUMBER(4),
  region_id           NVARCHAR2(36),
  win_status          NUMBER(6),
  tip_time            DATE,
  is_fault            NUMBER(4),
  CREATED_BY          NVARCHAR2(36),
  created_client      NVARCHAR2(50),
  CREATED_TIME        DATE,
  MODIFIED_BY         NVARCHAR2(36),
  last_updated_client NVARCHAR2(50),
  MODIFIED_TIME       DATE,
  e_code              NVARCHAR2(50) not null,
  entity_name         NVARCHAR2(100),
  source_id           NVARCHAR2(36),
  template_name       NVARCHAR2(255),
  org_id              NVARCHAR2(36)
)
;
