prompt Creating table sso_ticket
prompt =========================
prompt
create table sso_ticket
(
  ID          NVARCHAR2(128) not null,
  TICKET_ID   NVARCHAR2(128),
  TICKET      BLOB,
  TICKET_TYPE NVARCHAR2(128),
  CREATE_TIME DATE,
  HOST_NAME   NCLOB
)
;
COMMENT ON COLUMN sso_ticket.ID IS '票据ID';
COMMENT ON COLUMN sso_ticket.TICKET_ID IS '票据ID';
COMMENT ON COLUMN sso_ticket.TICKET IS '票据';
COMMENT ON COLUMN sso_ticket.TICKET_TYPE IS '票据类型';
COMMENT ON COLUMN sso_ticket.CREATE_TIME IS '创建时间';
COMMENT ON COLUMN sso_ticket.HOST_NAME IS '主机名';

