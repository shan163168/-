(
  id             NVARCHAR2(38) not null,
  file_id        NVARCHAR2(38) not null,
  type_y           NUMBER(6),
  status         NUMBER(6),
  source_code    NVARCHAR2(10),
  source_file_id NVARCHAR2(38),
  created_by     NVARCHAR2(50),
  created_date   DATE,
  e_code         NVARCHAR2(38)
)
;
