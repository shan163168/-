prompt Creating table sec_permission
prompt =============================
prompt
create table sec_permission
(
  ID                NVARCHAR2(36) not null,
  PARENT            NVARCHAR2(36),
  CODE              NVARCHAR2(100) not null,
  NAME              NVARCHAR2(255) not null,
  URL               NVARCHAR2(300),
  TYPE              NVARCHAR2(20) not null,
  ELEMENT_ID        NVARCHAR2(50),
  ELEMENT_BEHAVIOUR NVARCHAR2(30),
  ORDER_Y             NUMBER(11) not null,
  DESCRIPTION       NVARCHAR2(1000),
  CREATED_BY        NVARCHAR2(36) not null,
  CREATED_DATE      DATE not null,
  LAST_UPDATED_BY   NVARCHAR2(36) not null,
  LAST_UPDATED_DATE DATE not null,
  THUMBNAIL         NVARCHAR2(300),
  ICON              NVARCHAR2(300),
  ICON_LARGE        NVARCHAR2(300),
  ICON_MIDDLE       NVARCHAR2(300),
  ICON_SMALL        NVARCHAR2(300),
  SUBTYPE           NVARCHAR2(20),
  STATUS            NUMBER(11),
  WIDTH             NVARCHAR2(20),
  HEIGHT            NVARCHAR2(20),
  CONFIGURL         NVARCHAR2(300),
  WIDGET_TYPE       NVARCHAR2(20),
  RENDERTYPE        NVARCHAR2(20),
  APP_ID            NVARCHAR2(36),
  IS_REFERENCE      NUMBER(4),
  REFERENCE         NVARCHAR2(50),
  use_scope         NUMBER(4),
  application_code  NVARCHAR2(20),
  e_code            NVARCHAR2(50) not null
)
;
comment on table sec_permission
  is '操作权限表';
comment on column sec_permission.ID
  is '操作标识';
comment on column sec_permission.PARENT
  is '父操作标识';
comment on column sec_permission.CODE
  is '操作代码';
comment on column sec_permission.NAME
  is '名称';
comment on column sec_permission.URL
  is 'Page对应的URL，或者页面元素所在的URL';
comment on column sec_permission.TYPE
  is '模块、页面或者元素';
comment on column sec_permission.ELEMENT_ID
  is '受控页面元素的ID';
comment on column sec_permission.ELEMENT_BEHAVIOUR
  is '页面元素默认行为';
comment on column sec_permission.ORDER_Y
  is '顺序';
comment on column sec_permission.DESCRIPTION
  is '描述';
comment on column sec_permission.CREATED_BY
  is '创建人';
comment on column sec_permission.CREATED_DATE
  is '创建时间';
comment on column sec_permission.LAST_UPDATED_BY
  is '最后修改人';
comment on column sec_permission.LAST_UPDATED_DATE
  is '最后修改时间';
comment on column sec_permission.use_scope
  is '使用范围(0：服务端，1：服务端和终端)';
comment on column sec_permission.application_code
  is '所属应用模块';
comment on column sec_permission.e_code
  is '企业编码';



