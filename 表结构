(
  name           NVARCHAR2(38) not null,
  display_name   NVARCHAR2(150),
  driver_class   NVARCHAR2(350) not null,
  url            NVARCHAR2(350) not null,
  username       NVARCHAR2(38),
  password       NVARCHAR2(38),
  initial_size   NUMBER(11) not null,
  max_active     NUMBER(11) not null,
  max_idle       NUMBER(11) not null,
  min_idle       NUMBER(11) not null,
  timeout        NUMBER(11) not null,
  extra_schemas  NVARCHAR2(200),
  properties     NVARCHAR2(1000),
  is_system      NVARCHAR2(1),
  is_display     NVARCHAR2(1),
  display_for    NVARCHAR2(150),
  display_order  NUMBER(11),
  datastore_type NVARCHAR2(38) not null,
  summary        NVARCHAR2(1000),
  description    NCLOB,
  creation_time  DATE not null,
  last_modified  DATE not null
)
;
