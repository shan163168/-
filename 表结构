prompt =========================
prompt
create table sys_config
(
  id           NVARCHAR2(38) not null,
  category     NVARCHAR2(45),
  name         NVARCHAR2(100) not null,
  value        NCLOB,
  description  NVARCHAR2(500),
  order_no     NUMBER(11),
  is_important NVARCHAR2(1),
  is_deleted   NUMBER(4),
  scope        NVARCHAR2(300),
  config_file  NVARCHAR2(100)
)
;
comment on column sys_config.id
  is '配置id';
