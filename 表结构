create table sec_permission_rule
(
  ID                NVARCHAR2(36) not null,
  OPERATION_ID      NVARCHAR2(36),
  NAME              NVARCHAR2(255) not null,
  PRIORITY          NUMBER(11) not null,
  entity            NVARCHAR2(36),
  RULE              NVARCHAR2(1000) not null,
  BEHAVIOUR         NVARCHAR2(30),
  SCOPE_TYPE        NUMBER(11),
  DESCRIPTION       NVARCHAR2(1000),
  Is_Default        NUMBER(11) not null,
  CREATED_BY        NVARCHAR2(36) not null,
  Created_Date      DATE not null,
  LAST_UPDATED_BY   NVARCHAR2(36) not null,
  Last_Updated_Date DATE not null,
  e_code            NVARCHAR2(50) not null
)
;
comment on table sec_permission_rule
  is '权限规则表';
