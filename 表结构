prompt Creating table aip_desk_folder
prompt ==============================
prompt
create table aip_desk_folder
(
  id                NVARCHAR2(36) not null,
  name              NVARCHAR2(100) not null,
  desk_id           NVARCHAR2(36) not null,
  belong_to         NVARCHAR2(36),
  top               NVARCHAR2(10),
  left              NVARCHAR2(10),
  right             NVARCHAR2(10),
  display_order     NUMBER(11),
  is_deleted        NUMBER(4),
  created_by        NVARCHAR2(50),
  created_date      DATE,
  last_updated_by   NVARCHAR2(50),
  last_updated_date DATE,
  e_code            NVARCHAR2(50)
)
;
comment on table aip_desk_folder
  is '桌面文件夹';
comment on column aip_desk_folder.name
  is '文件夹名称';
comment on column aip_desk_folder.desk_id
  is '桌面ID';
comment on column aip_desk_folder.top
  is '文件夹坐标';
comment on column aip_desk_folder.left
  is '文件夹坐标';
comment on column aip_desk_folder.right
  is '文件夹坐标';
comment on column aip_desk_folder.display_order
  is '显示顺序';
comment on column aip_desk_folder.created_by
  is '创建者';
