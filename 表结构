  message_push      NVARCHAR2(2),
  is_deleted        NVARCHAR2(2),
  created_by        NVARCHAR2(36),
  created_date      DATE,
  last_updated_by   NVARCHAR2(36),
  last_updated_date DATE,
  e_code            NVARCHAR2(50)
)
