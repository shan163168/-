prompt Creating table aip_desk_ext
prompt ===========================
prompt
create table aip_desk_ext
(
  PERMISSION_ID NVARCHAR2(36) not null,
  DESK_TYPE     NVARCHAR2(20),
  STYLE_CLASS   NVARCHAR2(300),
  FUNC          NVARCHAR2(1000),
  DISPLAY_GROUP NVARCHAR2(255),
  VALIDATE_Y      NVARCHAR2(500),
  e_code        NVARCHAR2(50) not null
)
;
comment on column aip_desk_ext.FUNC
  is 'js脚本，如：webosService.quit() ;';
comment on column aip_desk_ext.e_code
  is '企业编码';
alter table aip_desk_ext
  add primary key (PERMISSION_ID);

