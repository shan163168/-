prompt Creating table blog_info
prompt ========================
prompt
create table blog_info
(
  blog_id               NVARCHAR2(38) not null,
  author_id             NVARCHAR2(38) not null,
  account_type          NVARCHAR2(2) not null,
  account_instance_id   NVARCHAR2(38) not null,
  content               NCLOB,
  commented_count       NUMBER(11),
  transfered_count      NUMBER(11),
  publish_time          DATE,
  src_blog_id           NVARCHAR2(38),
  base_blog_id          NVARCHAR2(38),
  is_forward            NUMBER(4),
  is_comment            NUMBER(4),
  is_private            NUMBER(4),
  private_type          NUMBER(4),
  private_group         NVARCHAR2(38),
  private_department    NVARCHAR2(38),
  blog_from_type        NUMBER(4),
  blog_from_display     NVARCHAR2(100),
  blog_from             NVARCHAR2(38),
  location              NVARCHAR2(100),
  private_from_name     NVARCHAR2(100),
  favorite_count        NUMBER(11),
  reply_comment_blog_id NVARCHAR2(38),
  is_notifycation       NUMBER(4),
  praise_count          NUMBER(11),
  is_deleted            NUMBER(4),
  at_account_ids        NCLOB,
  e_code                NVARCHAR2(50) not null,
  source_id             NVARCHAR2(36),
  entity_name           NVARCHAR2(100),
  approval_id           NVARCHAR2(36),
  org_id                NVARCHAR2(36),
  is_essential          NUMBER(4),
  has_index             NUMBER(4),
  action_params         NVARCHAR2(1000),
  source_name           NVARCHAR2(100),
  source_type           NUMBER(4),
  blog_scope_type       NUMBER(4),
  last_updated_by       NVARCHAR2(36),
  last_updated_date     DATE,
  src_comment_id        NVARCHAR2(36)
)
;
comment on column blog_info.author_id
  is '创建博客的用户';
comment on column blog_info.commented_count
  is '只增加，不删除';
comment on column blog_info.transfered_count
  is '只增不减';
comment on column blog_info.is_forward
  is '0：转发
            1：评论';
comment on column blog_info.is_private
  is '0：公开（默认）
            1：私密 ';
comment on column blog_info.private_type
  is '0：群组（群组微博）
            1：部门（部门微博）
            2：个人 ';
comment on column blog_info.private_group
  is '群组微博，这里群组微博只能归属于一个微博';
comment on column blog_info.private_department
  is '部门博客，这里微博只能归属于一个部门';
comment on column blog_info.blog_from_type
  is '0：系统内（默认）
            1：其他应用';
comment on column blog_info.blog_from
  is '这里主要是考虑企业内时，可以和具体业务系统对接';
comment on column blog_info.praise_count
  is '点赞次数';
comment on column blog_info.is_deleted
  is '动态是否被删除';
comment on column blog_info.at_account_ids
  is 'at的帐号Ids';
comment on column blog_info.e_code
  is '企业编码';
comment on column blog_info.source_id
  is '来源id';
comment on column blog_info.entity_name
  is '实体名';
comment on column blog_info.approval_id
  is '数据主页表id';
comment on column blog_info.org_id
  is '部门id';
comment on column blog_info.is_essential
  is '是否精华';
comment on column blog_info.has_index
  is '是否已建索引';
comment on column blog_info.action_params
  is '事件触发参数';
comment on column blog_info.source_name
  is '项目名称';
comment on column blog_info.source_type
  is '来源类型';
comment on column blog_info.blog_scope_type
  is '动态范围类型，1：完全可见，2：同事可见，3：好友可见，4：部分可见，5：私密范围';
comment on column blog_info.last_updated_by
  is '最后更新用户';
comment on column blog_info.last_updated_date
  is '最后更新时间';
comment on column blog_info.src_comment_id
  is '源评论ID';
alter table blog_info
  add primary key (blog_id);

