  COMMAND_ID        NVARCHAR2(36) not null,
  ACCOUNT_ID        NVARCHAR2(36) not null,
  ACTION_ID         NVARCHAR2(36),
  COMMAND           NVARCHAR2(50) not null,
  EVENT_SUB_ID      NVARCHAR2(38),
  IS_DEFAULT        NUMBER(4) not null,
  COMMAND_DESC      NVARCHAR2(1000),
  CREATED_BY        NVARCHAR2(36),
  created_date      DATE,
  LAST_UPDATED_BY   NVARCHAR2(36),
  last_updated_date DATE,
  ORDER_NO          NUMBER(11),
  IS_WELCOME        NUMBER(4) not null,
  e_code            NVARCHAR2(50) not null
)
;
COMMENT ON COLUMN sm_ivr_command.COMMAND_ID IS '指令ID';
COMMENT ON COLUMN sm_ivr_command.ACCOUNT_ID IS '账户ID';
COMMENT ON COLUMN sm_ivr_command.ACTION_ID IS '动作ID';
COMMENT ON COLUMN sm_ivr_command.COMMAND IS '指令';
COMMENT ON COLUMN sm_ivr_command.EVENT_SUB_ID IS '事件子ID';
COMMENT ON COLUMN sm_ivr_command.IS_DEFAULT IS '是否默认指令';
COMMENT ON COLUMN sm_ivr_command.COMMAND_DESC IS '指令描述';
COMMENT ON COLUMN sm_ivr_command.CREATED_BY IS '创建者ID';
COMMENT ON COLUMN sm_ivr_command.CREATED_DATE IS '创建日期';
COMMENT ON COLUMN sm_ivr_command.LAST_UPDATED_BY IS '最后更新者ID';
COMMENT ON COLUMN sm_ivr_command.LAST_UPDATED_DATE IS '最后更新日期';
COMMENT ON COLUMN sm_ivr_command.ORDER_NO IS '排序号';
COMMENT ON COLUMN sm_ivr_command.IS_WELCOME IS '是否欢迎指令';
COMMENT ON COLUMN sm_ivr_command.e_code IS 'e码';

