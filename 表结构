prompt
prompt Creating table uam_group
prompt ========================
prompt
create table uam_group
(
  GROUP_ID            NVARCHAR2(100) not null,
  TYPE_ID             NVARCHAR2(36),
  name                NVARCHAR2(255),
  ICON                NVARCHAR2(300),
  STATUS              NUMBER(11),
  TYPE_Y                NUMBER(6),
  subject_id          NVARCHAR2(36),
  IS_PUBLIC           NUMBER(6),
  OWNER               NVARCHAR2(100),
  SOURCE              NVARCHAR2(255),
  IS_STICKY           NUMBER(4),
  ORG_ID              NVARCHAR2(36),
  silent_status       NUMBER(6) not null,
  template_id         NVARCHAR2(36),
  is_allow_mem_invite NUMBER(4) not null,
  join_mode           NUMBER(4) not null,
  CREATED_BY          NVARCHAR2(100),
  CREATED_DATE        DATE,
  LAST_UPDATED_BY     NVARCHAR2(100),
  LAST_UPDATED_DATE   DATE,
  memo                NVARCHAR2(500),
  DISK_QUOTA          NUMBER(11),
  IS_OPEN             NUMBER(6),
  name_pinyin         NVARCHAR2(500),
  name_py             NVARCHAR2(100),
  IS_DELETED          NUMBER(4),
  MAX_USER_COUNT      NUMBER(11),
  IS_RECEIVE_CHECKIN  NUMBER(6),
  IS_ALLOW_SELFEXIT   NUMBER(11),
  enable_face_msg     NUMBER(4),
  S_CODE              NVARCHAR2(50),
  S_ID                NVARCHAR2(36),
  E_Code              NVARCHAR2(50),
  BULLETIN_COUNT      NUMBER(11),
  BULLETIN_SHOW_TYPE  NUMBER(6),
  IS_FORCE_RECEIVE    NUMBER(4),
  IS_THIRD_GROUP      NUMBER(4),
  action_params       NVARCHAR2(1000),
  related_account_id  NVARCHAR2(50),
  extend_params       NVARCHAR2(500)
)
;
comment on table uam_group
  is '群组';
comment on column uam_group.GROUP_ID
  is '编号';
comment on column uam_group.TYPE_ID
  is '类型编号';
comment on column uam_group.name
  is '名称';
comment on column uam_group.ICON
  is '群组图标';
comment on column uam_group.STATUS
  is '状态';
comment on column uam_group.TYPE_Y
  is '群组类型1:内部群组2:外部群组3:项目群组';
comment on column uam_group.subject_id
  is '群组主体ID';
comment on column uam_group.OWNER
  is '所有人';
comment on column uam_group.SOURCE
  is '创建来源（系统 编号）';
comment on column uam_group.IS_STICKY
  is '是否置顶（0：否，1：是）';
comment on column uam_group.ORG_ID
  is '群组所属组织';
comment on column uam_group.silent_status
  is '禁言状态（0:不禁言,1:全体禁言）';
comment on column uam_group.template_id
  is '关联的群模版';
comment on column uam_group.is_allow_mem_invite
  is '是否允许成员邀请用户（0:不允许，1:允许）';
comment on column uam_group.join_mode
  is '入群方式（0:不限制，1:需要群主或者管理员审核）';
comment on column uam_group.CREATED_BY
  is '创建人';
comment on column uam_group.CREATED_DATE
  is '创建时间';
comment on column uam_group.LAST_UPDATED_BY
  is '最近修改人';
comment on column uam_group.LAST_UPDATED_DATE
  is '最近修改时间';
comment on column uam_group.memo
  is '备注';
comment on column uam_group.MAX_USER_COUNT
  is '群组用户最大数';
comment on column uam_group.IS_ALLOW_SELFEXIT
  is '是否允许自行退群';
comment on column uam_group.enable_face_msg
  is '是否启用表情消息';
comment on column uam_group.S_CODE
  is '数据源企业Code';
comment on column uam_group.S_ID
  is '源数据的ID';
comment on column uam_group.BULLETIN_COUNT
  is '公告总数';
comment on column uam_group.BULLETIN_SHOW_TYPE
  is '公告显示方式';
comment on column uam_group.IS_FORCE_RECEIVE
  is '是否强制接收消息';
comment on column uam_group.IS_THIRD_GROUP
  is '是否第三方群组';
comment on column uam_group.action_params
  is '事件触发参数';
comment on column uam_group.related_account_id
  is '关联的服务号';
comment on column uam_group.extend_params
  is '扩展参数';

