create table temp_sm_service
(
  ID             NVARCHAR2(144),
  NAME           NCLOB,
  CATEGORY       NVARCHAR2(144),
  ORDER_NO       NUMBER(11),
  TYPE_Y           NUMBER(4),
  RECOMMEND      NUMBER(11),
  USED_COUNT     NUMBER(11),
  FAVORITE_COUNT NUMBER(11),
  ICON           NCLOB,
  ICON2          NCLOB,
  DESCRIPTION    NCLOB,
  STATUS         NUMBER(11),
  PARAMS         NCLOB,
  TERMINAL_TYPE  NUMBER(11),
  ACTION_PARAMS  NCLOB,
  AUTH_TYPE      NUMBER(4),
  CREATED_BY     NVARCHAR2(144),
  CREATED_TIME   DATE,
  MODIFIED_BY    NVARCHAR2(144),
  MODIFIED_TIME  DATE,
  E_CODE         NVARCHAR2(400),
  entity_name    NVARCHAR2(400)
)
;
comment on table temp_sm_service is '临时服务号记录表';
comment on column temp_sm_service.id is '主键ID';
comment on column temp_sm_service.NAME is '名称';
comment on column temp_sm_service.CATEGORY is '类型';
comment on column temp_sm_service.ORDER_NO is '序号';
comment on column temp_sm_service.TYPE_Y is '类型';
comment on column temp_sm_service.RECOMMEND is '回复';
comment on column temp_sm_service.USED_COUNT is '用户数量';
comment on column temp_sm_service.FAVORITE_COUNT is '收藏数量';
comment on column temp_sm_service.ICON is '图标1';
comment on column temp_sm_service.ICON2 is '图标2';
comment on column temp_sm_service.DESCRIPTION is '描述';
comment on column temp_sm_service.STATUS is '状态';
comment on column temp_sm_service.PARAMS is '参数';
comment on column temp_sm_service.TERMINAL_TYPE is '端子类型';
comment on column temp_sm_service.ACTION_PARAMS is '执行参数';
comment on column temp_sm_service.AUTH_TYPE is '认证类型';
comment on column temp_sm_service.E_CODE is '租户编码';
comment on column temp_sm_service.entity_name is '实体名称';
comment on column temp_sm_service.created_by is '创建人';
comment on column temp_sm_service.CREATED_TIME is '创建时间';
comment on column temp_sm_service.MODIFIED_BY is '最后更新者';
comment on column temp_sm_service.MODIFIED_TIME is '最后更新时间';


