prompt Creating table uam_external_user
prompt ================================
prompt
create table uam_external_user
(
  USER_ID           NVARCHAR2(50) not null,
  USER_NUM          NVARCHAR2(128),
  NAME              NVARCHAR2(64) not null,
  FULL_NAME         NVARCHAR2(255),
  LOGIN_ID          NVARCHAR2(50) not null,
  PASSWORD          NVARCHAR2(100),
  APP_PASSWORD      NVARCHAR2(100),
  ORG_ID            NVARCHAR2(50),
  DOMAIN            NVARCHAR2(50),
  TYPE_Y              NUMBER(6),
  STATUS            NUMBER(6),
  SEX               NUMBER(6),
  RANK              NUMBER(11),
  EMAIL             NVARCHAR2(255),
  MOBILE            NVARCHAR2(255),
  IM                NVARCHAR2(255),
  TELEPHONE         NVARCHAR2(255),
  BIRTHDAY          DATE,
  PICTURE           NVARCHAR2(255),
  ORDER_NO          NUMBER(11),
  DESCRIPTION       NVARCHAR2(1000),
  CREATED_DATE      DATE,
  LAST_UPDATED_DATE DATE,
  mobile2           NVARCHAR2(500),
  mobile3           NVARCHAR2(500),
  homeTelephone     NVARCHAR2(500),
  owner             NVARCHAR2(500),
  sourceApp         NVARCHAR2(500),
  createdBy         NVARCHAR2(50),
  lastUpdatedBy     NVARCHAR2(50),
  e_code            NVARCHAR2(50) not null
)
;
comment on table uam_external_user
  is '外部用户';
