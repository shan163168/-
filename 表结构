prompt
prompt Creating table am_app
prompt =====================
prompt
create table am_app
(
  APP_ID              NVARCHAR2(36) not null,
  ISV_ID              NVARCHAR2(36),
  ISV_NAME            NVARCHAR2(255),
  CODE                NVARCHAR2(50) not null,
  NAME                NVARCHAR2(255) not null,
  IS_ENABLED          NUMBER(4),
  BIG_IMG             NVARCHAR2(300),
  SMALL_IMG           NVARCHAR2(300),
  IS_SYSTEM           NUMBER(4) not null,
  SUPPORT_APP         NVARCHAR2(36),
  DESCRIPTION         NVARCHAR2(1000),
  BELONG_DEPT         NVARCHAR2(36),
  SERVER_CHARGER      NVARCHAR2(36),
  BACKUP_CHARGER      NVARCHAR2(36),
  client_id           NVARCHAR2(36),
  client_secret       NVARCHAR2(50),
  region_id           NVARCHAR2(36),
  CREATED_BY          NVARCHAR2(36),
  created_client      NVARCHAR2(50),
  created_date        DATE,
  LAST_UPDATED_BY     NVARCHAR2(36),
  last_updated_client NVARCHAR2(50),
  last_updated_date   DATE,
  external_id         NVARCHAR2(36),
  e_code              NVARCHAR2(50) not null,
  is_preinstall       NUMBER(4),
  category            NVARCHAR2(36),
  source_id           NVARCHAR2(36),
  org_id              NVARCHAR2(36)
)
;
comment on column am_app.ISV_NAME
  is '供应商名称';
comment on column am_app.CODE
  is '同一种应用的编号一样，例如会议模块
            有Web应用，有iphone应用，有android应用，他们的编号是一样的';
