prompt Creating table aip_portal
prompt =========================
prompt
create table aip_portal
(
  PORTAL_ID        NVARCHAR2(36) not null,
  CODE             NVARCHAR2(50) not null,
  NAME             NVARCHAR2(255) not null,
  STATUS           NVARCHAR2(10) not null,
  DESCRIPTION      NVARCHAR2(1000),
  ACCESS_TIMES     NUMBER(20),
  CREATE_USER      NVARCHAR2(36),
  CREATE_TIME      DATE,
  LAST_UPDATE_USER NVARCHAR2(36),
  LAST_UPDATE_TIME DATE,
  LAYOUT_URL       NVARCHAR2(300),
  LOGO_URL         NVARCHAR2(300),
  FOOTER           NVARCHAR2(500),
  THEME            NVARCHAR2(500),
  TITLE            NVARCHAR2(500),
  ACCESS_URL       NVARCHAR2(300),
  THUMBNAIL_URL    NVARCHAR2(300),
  LAYOUT_ID        NVARCHAR2(36),
  DEV_CATEGORY_ID  NVARCHAR2(36),
  SYSTEM_ID        NVARCHAR2(36),
  INSTALL_FLAG     NVARCHAR2(20),
  CATEGORY_ID      NVARCHAR2(36)
)
;
comment on table aip_portal
  is '视窗门户';
comment on column aip_portal.PORTAL_ID
  is '视窗标识';
comment on column aip_portal.CODE
  is '视窗代码';
comment on column aip_portal.NAME
  is '视窗名称';
comment on column aip_portal.STATUS
  is '视窗状态';
comment on column aip_portal.DESCRIPTION
  is '视窗描述';
comment on column aip_portal.ACCESS_TIMES
  is '视窗访问次数';
comment on column aip_portal.CREATE_USER
  is '创建用户';
comment on column aip_portal.CREATE_TIME
  is '创建时间';
comment on column aip_portal.LAST_UPDATE_USER
  is '最近更新用户';
comment on column aip_portal.LAST_UPDATE_TIME
  is '最近更新时间';
comment on column aip_portal.LOGO_URL
  is 'logo url';
comment on column aip_portal.FOOTER
  is '页脚';
comment on column aip_portal.THEME
  is '主题';
comment on column aip_portal.TITLE
  is '标题';
comment on column aip_portal.ACCESS_URL
  is '访问UrL';
comment on column aip_portal.THUMBNAIL_URL
  is '缩略图路径';
comment on column aip_portal.LAYOUT_ID
  is '布局框架ID';
alter table aip_portal
  add primary key (PORTAL_ID);

  
