prompt Creating table uam_group_bulletin
prompt =================================
prompt
create table uam_group_bulletin
(
  BULLETIN_ID       NVARCHAR2(36) not null,
  GROUP_ID          NVARCHAR2(36),
  title             NVARCHAR2(255),
  content           NVARCHAR2(1000),
  CREATED_TYPE      NUMBER(6),
  CREATED_BY        NVARCHAR2(36),
  CREATED_DATE      DATE,
  LAST_UPDATED_BY   NVARCHAR2(36),
  LAST_UPDATED_DATE DATE,
  e_code            NVARCHAR2(50) not null,
  is_important      NUMBER(4),
  from_client       NVARCHAR2(50)
)
;
comment on table uam_group_bulletin
  is '群组公告';
comment on column uam_group_bulletin.CREATED_TYPE
  is '创建者类型 0：用户 1：机器人';
comment on column uam_group_bulletin.e_code
  is '企业编码';
comment on column uam_group_bulletin.is_important
  is '是否重要公告';
comment on column uam_group_bulletin.from_client
  is '应用client';

