prompt Creating table oauth2_client
prompt ============================
prompt
create table oauth2_client
(
  id                   NVARCHAR2(38) not null,
  name                 NVARCHAR2(50),
  secret               NVARCHAR2(500),
  redirect_uri         NVARCHAR2(1000),
  redirect_uri_pattern NVARCHAR2(300),
  logout_uri           NVARCHAR2(1000),
  logout_uri_pattern   NVARCHAR2(300),
  at_expires           NUMBER(11),
  rt_expires           NUMBER(11),
  allow_authz_code     NVARCHAR2(1),
  allow_refresh_token  NVARCHAR2(1),
  allow_login_token    NVARCHAR2(1),
  enabled              NVARCHAR2(1),
  public_key           NCLOB,
  private_key          NCLOB,
  contacts             NVARCHAR2(50),
  phone_num            NVARCHAR2(20),
  email                NVARCHAR2(50),
  logo_uri             NVARCHAR2(500),
  description          NVARCHAR2(500),
  created_by           NVARCHAR2(38),
  updated_by           NVARCHAR2(38),
  created_at           DATE,
  updated_at           DATE,
  granted_scope        NVARCHAR2(1000),
  group_id             NVARCHAR2(38),
  is_internal          NVARCHAR2(1),
  scope_id             NVARCHAR2(38),
  org_id               NVARCHAR2(38),
  allow_use_password   NVARCHAR2(1),
  e_code               NVARCHAR2(50),
  type                 NVARCHAR2(20) not null,
  e_default            NUMBER(4) not null,
  owner_city           NVARCHAR2(20),
  owner_jingzhong      NVARCHAR2(20),
  person_liable        NVARCHAR2(38),
  project_id           NVARCHAR2(38),
  app_id               NVARCHAR2(38)
)
;
comment on column oauth2_client.scope_id
  is '应用管理权限对应的scope_id';
comment on column oauth2_client.org_id
  is '归属组织信息';
comment on column oauth2_client.type
  is '预留字段，外部可以根据业务需求使用这个字段对应用做分类，默认值client';
comment on column oauth2_client.e_default
  is '是否企业默认client,在sso互联模式下用于代表聆客在企业内的身份';

