prompt Creating table msg_session
prompt ==========================
prompt
create table msg_session
(
  id              NVARCHAR2(40) not null,
  user_id         NVARCHAR2(40) not null,
  from_id         NVARCHAR2(40) not null,
  from_name       NVARCHAR2(100),
  from_type       NUMBER(11),
  last_content    NCLOB,
  last_msg_type   NUMBER(11),
  last_time       NVARCHAR2(30),
  send_id         NVARCHAR2(40),
  send_name       NVARCHAR2(100),
  read_statue     NUMBER(4),
  last_msg_id     NVARCHAR2(40),
  last_time_stamp NVARCHAR2(20),
  e_code          NVARCHAR2(50) not null
)
;
-- 为表 msg_session 中的字段添加说明
COMMENT ON COLUMN msg_session.id IS '会话ID';
COMMENT ON COLUMN msg_session.user_id IS '用户ID';
COMMENT ON COLUMN msg_session.from_id IS '消息发送者ID';
COMMENT ON COLUMN msg_session.from_name IS '消息发送者姓名';
COMMENT ON COLUMN msg_session.from_type IS '消息发送者类型';
COMMENT ON COLUMN msg_session.last_content IS '最后一条消息内容';
COMMENT ON COLUMN msg_session.last_msg_type IS '最后一条消息类型';
COMMENT ON COLUMN msg_session.last_time IS '最后一条消息时间';
COMMENT ON COLUMN msg_session.send_id IS '消息发送者ID';
COMMENT ON COLUMN msg_session.send_name IS '消息发送者姓名';
COMMENT ON COLUMN msg_session.last_msg_id IS '最后一条消息ID';
COMMENT ON COLUMN msg_session.last_time_stamp IS '最后一条消息时间戳';

comment on column msg_session.read_statue
  is '消息阅读状态（0:有未读消息,1:没有未读消息）';
comment on column msg_session.e_code
  is '企业编码';
alter table msg_session
  add primary key (user_id, from_id);

