create table tmp_user
(
  id            NVARCHAR2(200) not null,
  idcard        NVARCHAR2(200),
  userName      NVARCHAR2(200),
  policeNo      NVARCHAR2(200),
  orgCode       NVARCHAR2(200),
  orgName       NVARCHAR2(200),
  telephone     NVARCHAR2(100),
  mobile        NVARCHAR2(100),
  phone         NVARCHAR2(100),
  nation        NVARCHAR2(100),
  personType    NVARCHAR2(10),
  policeRank    NVARCHAR2(100),
  dutylevel     NVARCHAR2(100),
  appoint       NVARCHAR2(100),
  personPost    NVARCHAR2(100),
  isJob         NVARCHAR2(10),
  policeType    NVARCHAR2(200),
  bloodGroup    NVARCHAR2(100),
  photo         BLOB,
  auxiliaryType NVARCHAR2(10),
  sortNum       NVARCHAR2(100),
  isDeleted     NUMBER(4),
  sex           NVARCHAR2(10),
  orgs          NCLOB,
  updateTime    DATE,
  status        NUMBER(4),
  initPassword  NVARCHAR2(100),
  smsStatus     NUMBER(4)
)
;
