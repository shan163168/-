create table pan_file
(
  file_id        NVARCHAR2(255) not null,
  dir_id         NVARCHAR2(255),
  file_name      NVARCHAR2(255) not null,
  file_ext       NVARCHAR2(255) not null,
  file_size      NUMBER(20) not null,
  version        NUMBER(11) not null,
  principal_type NVARCHAR2(255) not null,
  principal_id   NVARCHAR2(255) not null,
  creator_id     NVARCHAR2(255) not null,
  created_at     DATE not null,
  updated_at     DATE not null,
  is_deleted     NVARCHAR2(1) not null,
  scope_id       NVARCHAR2(50),
  idx_state      NUMBER(20),
  idx_time       NUMBER(20),
  cvt_state      NUMBER(20),
  updator_id     NVARCHAR2(50),
  is_open        NVARCHAR2(1) not null,
  locked         NUMBER(4) not null,
  locked_at      DATE,
  locked_by      NVARCHAR2(50),
  file_hash      NVARCHAR2(255),
  s3_info_id     NVARCHAR2(255)
)
;
