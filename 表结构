prompt
create table am_terminal_app
(
  SUB_APP_ID           NVARCHAR2(36) not null,
  ISV_ID               NVARCHAR2(36),
  APP_TERMINAL_ID      NVARCHAR2(36),
  CODE                 NVARCHAR2(50),
  NAME                 NVARCHAR2(255),
  APP_ACCOUNT          NVARCHAR2(100),
  ACCESS_KEY           NVARCHAR2(100),
  SECRET_KEY           NVARCHAR2(300),
  BIG_IMG              NVARCHAR2(300),
  SMALL_IMG            NVARCHAR2(300),
  IS_ENABLED           NUMBER(4),
  BELONG_DEPT          NVARCHAR2(36),
  BELONG_GROUP         NVARCHAR2(36),
  SERVER_CHARGER       NVARCHAR2(36),
  BACKUP_CHARGER       NVARCHAR2(36),
  FAULT_REPSONSE_LEVEL NVARCHAR2(20),
  DESCRIPTION          NVARCHAR2(1000),
  CREATED_BY           NVARCHAR2(36),
  created_client       NVARCHAR2(50),
  created_date         DATE,
  LAST_UPDATED_BY      NVARCHAR2(36),
  last_updated_client  NVARCHAR2(50),
  last_updated_date    DATE,
  external_id          NVARCHAR2(36),
  VERSION_ID           NVARCHAR2(36),
  IS_HIDDEN            NUMBER(6),
  app_id               NVARCHAR2(36),
  e_code               NVARCHAR2(50) not null,
  source_id            NVARCHAR2(36)
)
;
comment on table am_terminal_app
  is '终端子应用，每一个终端都可能对应多个应用，如应用OA，可能对应更多个Android应用：日程管理、会议管理。';
comment on column am_terminal_app.APP_ACCOUNT
  is '对用用户中的guid码，用以进行服务端访问授权时如果对于数据权限有要求时，可以根据此进行权限设置';
comment on column am_terminal_app.created_client
  is '创建的client';
comment on column am_terminal_app.last_updated_client
  is '最后更新的client';
comment on column am_terminal_app.external_id
  is '外部数据源ID';
comment on column am_terminal_app.e_code
  is '企业编码';
comment on column am_terminal_app.source_id
  is '源ID';
alter table am_terminal_app
  add primary key (SUB_APP_ID);

prompt
prompt Creating table am_user_apps
prompt ===========================
prompt
create table am_user_apps
(
  APP_ID            NVARCHAR2(36) not null,
  USER_ID           NVARCHAR2(36) not null,
  created_time      DATE,
  CREATED_BY        NVARCHAR2(36),
  LAST_UPDATED_BY   NVARCHAR2(36),
  last_updated_date DATE,
  e_code            NVARCHAR2(50) not null
)
;
COMMENT ON COLUMN am_user_apps.APP_ID IS '应用程序ID';
COMMENT ON COLUMN am_user_apps.USER_ID IS '用户ID';
COMMENT ON COLUMN am_user_apps.created_time IS '创建时间';
COMMENT ON COLUMN am_user_apps.CREATED_BY IS '创建人';
COMMENT ON COLUMN am_user_apps.LAST_UPDATED_BY IS '最后更新人';
COMMENT ON COLUMN am_user_apps.last_updated_date IS '最后更新日期';
COMMENT ON COLUMN am_user_apps.e_code IS '租户编码';

alter table am_user_apps
  add primary key (APP_ID, USER_ID);

prompt
prompt Creating table analysis_log
prompt ===========================
prompt
create table analysis_log
(
  ID              NVARCHAR2(38) not null,
  DEVICE_ID       NVARCHAR2(50),
  DEVICE_NAME     NVARCHAR2(100),
  MODULE_CATEGORY NVARCHAR2(50) not null,
  MODULE_NAME     NVARCHAR2(100) not null,
  EVENT_TYPE      NVARCHAR2(45) not null,
  EVENT_PARAMS    NCLOB,
  EVENT_ENTRY     NVARCHAR2(50),
  EVENT_TIME      DATE not null,
  TARGET_ID       NVARCHAR2(38),
  TARGET_NAME     NVARCHAR2(50),
  OPERATOR_ID     NVARCHAR2(38),
  OPERATOR_NAME   NVARCHAR2(50),
  RECORD_TIME     DATE not null,
  E_CODE          NVARCHAR2(45),
  device_type     NVARCHAR2(10)
)
;
comment on table analysis_log
  is '用户行为统计日志';
comment on column analysis_log.ID
  is '编号';
comment on column analysis_log.DEVICE_ID
  is '设备ID';
comment on column analysis_log.DEVICE_NAME
  is '设备名称';
comment on column analysis_log.MODULE_CATEGORY
  is '模块类别
服务、服务号、登陆、设备管理、部门、用户、群组、动态';
comment on column analysis_log.MODULE_NAME
  is '所属模块';
comment on column analysis_log.EVENT_TYPE
  is '事件类型';
comment on column analysis_log.EVENT_PARAMS
  is '事件参数';
comment on column analysis_log.EVENT_ENTRY
  is '事件入口';
comment on column analysis_log.EVENT_TIME
  is '事件发生时间';
comment on column analysis_log.TARGET_ID
  is '目标ID';
comment on column analysis_log.TARGET_NAME
  is '目标名称';
comment on column analysis_log.OPERATOR_ID
  is '操作者ID';
comment on column analysis_log.OPERATOR_NAME
  is '操作者名称';
comment on column analysis_log.RECORD_TIME
  is '记录入库时间';
comment on column analysis_log.E_CODE
  is '所属企业code';
comment on column analysis_log.device_type
  is '设备类型';
alter table analysis_log
  add primary key (ID);

