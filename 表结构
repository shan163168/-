  FULL_NAME         NVARCHAR2(255),
  alias             NVARCHAR2(255),
  LOGIN_ID          NVARCHAR2(50),
  PASSWORD          NVARCHAR2(255),
  ORG_ID            NVARCHAR2(36),
  DOMAIN            NVARCHAR2(50),
  TYPE_Y              NUMBER(6),
  STATUS            NUMBER(11),
  SEX               NUMBER(6),
  RANK              NUMBER(11),
  EMAIL             NVARCHAR2(50),
  MOBILE            NVARCHAR2(60),
  IM                NVARCHAR2(50),
  TELEPHONE         NVARCHAR2(255),
  BIRTHDAY          DATE,
  PICTURE           NVARCHAR2(300),
  ORDER_NO          NUMBER(11) not null,
  DESCRIPTION       NVARCHAR2(1000),
  control_client    NVARCHAR2(50),
  is_hidden         NUMBER(4),
  is_active         NUMBER(4),
  identity_num      NVARCHAR2(20),
  extend_fields     NCLOB,
  work_status       NVARCHAR2(20),
