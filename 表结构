prompt Creating table uam_organization
prompt ===============================
prompt
create table uam_organization
(
  ORG_ID            NVARCHAR2(36) not null,
  ORG_NUM           NVARCHAR2(50),
  name              NVARCHAR2(255) not null,
  full_name         NVARCHAR2(255),
  PARENT_ID         NVARCHAR2(36),
  CODE              NVARCHAR2(500),
  level_y             NUMBER(11),
  TYPE_Y              NUMBER(6),
  STATUS            NUMBER(11) not null,
  ORDER_NO          NUMBER(11) not null,
  description       NVARCHAR2(1000),
  ICODE             NVARCHAR2(50),
  is_hidden         NUMBER(4),
  is_independent    NUMBER(4),
  CREATED_DATE      DATE,
  LAST_UPDATED_DATE DATE,
  CREATED_BY        NVARCHAR2(36),
  LAST_UPDATED_BY   NVARCHAR2(36),
  DISK_QUOTA        NUMBER(11),
  name_pinyin       NVARCHAR2(150),
  name_py           NVARCHAR2(100),
  TELEPHONE         NVARCHAR2(20),
  email             NVARCHAR2(100),
  ICON              NVARCHAR2(300),
  IS_DELETED        NUMBER(4),
  ENABLE_CHAT       NUMBER(11),
  PRINCIPAL         NVARCHAR2(36),
  S_CODE            NVARCHAR2(50),
  E_CODE            NVARCHAR2(50),
  FULL_PATH_NAME    NVARCHAR2(500),
  ASYN_FlAG         NVARCHAR2(2),
  source            NVARCHAR2(10),
  Secret_level      NUMBER(6),
  IS_FORCE_RECEIVE  NUMBER(4),
  default_role      NVARCHAR2(38),
  source_id         NVARCHAR2(38),
  subject_id        NVARCHAR2(36)
)
;
comment on table uam_organization
  is '组织机构，组织架构的部门';
comment on column uam_organization.ORG_ID
  is '组织ID';
comment on column uam_organization.ORG_NUM
  is '组织编号';
comment on column uam_organization.name
  is '组织名';
