prompt Creating table aip_widget_ext
prompt =============================
prompt
create table aip_widget_ext
(
  PERMISSION_ID  NVARCHAR2(38) not null,
  CATEORY_ID     NVARCHAR2(38),
  SOURCE_TYPE    NVARCHAR2(254),
  SOURCE_ID      NVARCHAR2(38),
  HEIGHT         NVARCHAR2(10),
  STATUS         NVARCHAR2(10) not null,
  TITLE_URL      NVARCHAR2(350),
  AUTHOR_EMAIL   NVARCHAR2(100),
  AUTHOR         NVARCHAR2(100),
  SCREENSHOT     NVARCHAR2(500),
  WIDTH          NVARCHAR2(10),
  METADATA       NCLOB,
  CONFIG_URL     NVARCHAR2(350),
  PORTAL_ID      NVARCHAR2(50),
  ADD_CONFIG_URL NVARCHAR2(500)
)
;
comment on table aip_widget_ext
  is '视图构件';
comment on column aip_widget_ext.PERMISSION_ID
  is '构件标识';
comment on column aip_widget_ext.CATEORY_ID
  is '分类标识';
comment on column aip_widget_ext.SOURCE_TYPE
  is '来源类型';
comment on column aip_widget_ext.SOURCE_ID
  is '来源对象';
comment on column aip_widget_ext.HEIGHT
  is '默认高度';
comment on column aip_widget_ext.STATUS
  is '构件状态';
comment on column aip_widget_ext.SCREENSHOT
  is '给出小工具屏幕截图网址的可选字符串';
alter table aip_widget_ext
  add primary key (PERMISSION_ID);

