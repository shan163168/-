  external_id        NVARCHAR2(100),
  family_name        NVARCHAR2(30),
  given_name         NVARCHAR2(30),
  middle_name        NVARCHAR2(30),
  nick_name          NVARCHAR2(100),
  job_title          NVARCHAR2(100),
  email              NVARCHAR2(100),
  email_verified     NUMBER(4) not null,
  mobile             NVARCHAR2(20),
  mobile1            NVARCHAR2(20),
  mobile2            NVARCHAR2(20),
  phone_number       NVARCHAR2(30),
  employee_number    NVARCHAR2(20),
  gender             NCHAR(1) not null,
  birthdate          DATE,
  thumbnail          NVARCHAR2(1000),
  photo              NVARCHAR2(1000),
  locale             NVARCHAR2(20),
  timezone           NVARCHAR2(20),
  preferred_language NVARCHAR2(20),
  country            NVARCHAR2(30),
  ethnicity          NVARCHAR2(20),
  region             NVARCHAR2(30),
  locality           NVARCHAR2(50),
  street_address     NVARCHAR2(300),
  postal_code        NVARCHAR2(10),
  active             NUMBER(4) not null,
  is_deleted         NUMBER(4) not null,
  description        NCLOB,
