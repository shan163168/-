prompt Creating table blog_account
prompt ===========================
prompt
create table blog_account
(
  account_id          NVARCHAR2(38) not null,
  account_type        NVARCHAR2(2) not null,
  account_instance_id NVARCHAR2(38) not null,
  account_name        NVARCHAR2(300) not null,
  account_image       NVARCHAR2(300),
  account_remark      NVARCHAR2(255),
  account_background  NVARCHAR2(20),
  extra_info          NVARCHAR2(100),
  status              NUMBER(4),
  follow_count        NUMBER(11),
  funs_count          NUMBER(11),
  blog_count          NUMBER(11),
  favorite_blog_count NUMBER(11),
  last_access_time    DATE,
  created_date        DATE not null,
  last_updated_date   DATE not null,
  e_code              NVARCHAR2(50) not null
)
;
comment on column blog_account.account_id
  is '根据类型不同，关联不同的ID，因为用户、群组、部门的ID都是GUID，因此不会重复，所以这里直接用作帐号ID';
comment on column blog_account.account_name
  is '账号名称';
comment on column blog_account.account_image
  is '帐号图标';
comment on column blog_account.account_remark
  is '账号备注';
comment on column blog_account.extra_info
  is '额外信息';
comment on column blog_account.status
  is '表示当前博客帐号是否可用';
comment on column blog_account.last_access_time
  is '最后访问动态的时间';
comment on column blog_account.created_date
  is '帐号创建时间';
comment on column blog_account.last_updated_date
  is '帐号最后修改时间';
comment on column blog_account.e_code
  is '企业编码';
alter table blog_account
  add primary key (account_id);

