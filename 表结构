create table wf_user_role
(
  id                NVARCHAR2(36) not null,
  user_id           NVARCHAR2(36) not null,
  org_id            NVARCHAR2(36) not null,
  role_id           NVARCHAR2(36) not null,
  created_by        NVARCHAR2(50) not null,
  created_date      DATE not null,
  last_updated_by   NVARCHAR2(50) not null,
  last_updated_date DATE not null,
  E_CODE            NVARCHAR2(100)
)
;
