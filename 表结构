(
  SCHED_NAME        NVARCHAR2(120),
  JOB_NAME          NVARCHAR2(200) not null,
  JOB_GROUP         NVARCHAR2(200) not null,
  DESCRIPTION       NVARCHAR2(250),
  JOB_CLASS_NAME    NVARCHAR2(250) not null,
  IS_DURABLE        NVARCHAR2(1) not null,
  IS_NONCONCURRENT  NVARCHAR2(1),
  IS_UPDATE_DATA    NVARCHAR2(1),
  IS_VOLATILE       NVARCHAR2(1) not null,
  IS_STATEFUL       NVARCHAR2(1) not null,
  REQUESTS_RECOVERY NVARCHAR2(1) not null,
  JOB_DATA          BLOB
)
;
comment on column qrtz_job_details.SCHED_NAME is '调度器名称';
comment on column qrtz_job_details.JOB_NAME is '作业名称';
comment on column qrtz_job_details.JOB_GROUP is '作业组';
